{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "installCommand": "npm install",
  "framework": "vite",
  "outputDirectory": "frontend/dist",
  "env": {
    "VITE_API_BASE_URL": "@vite_api_base_url"
  },
  "regions": ["iad1"],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
